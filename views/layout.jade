		script(src='//cdnjs.cloudflare.com/ajax/libs/jquery-jsonview/1.2.3/jquery.jsonview.min.js')
		script(src='//src.remaxthailand.co.th/libs/adminlte/2.3.0/js/app.min.js')
		script(src='/javascripts/common.js')